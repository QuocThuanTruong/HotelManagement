<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:HotelManagement">

    <FontFamily x:Key="Bebas">pack://application:,,,/Assets/Fonts/UTM Bebas.ttf #UTM Bebas</FontFamily>

    <FontFamily x:Key="GilroyBold">pack://application:,,,/Assets/Fonts/SVN-Gilroy Bold.otf #SVN-Gilroy</FontFamily>
    <FontFamily x:Key="GilroyMedium">pack://application:,,,/Assets/Fonts/SVN-Gilroy Medium.otf #SVN-Gilroy</FontFamily>
    <FontFamily x:Key="GilroyRegular">pack://application:,,,/Assets/Fonts/SVN-Gilroy Regular.otf #SVN-Gilroy</FontFamily>
    <FontFamily x:Key="GilroySemiBold">pack://application:,,,/Assets/Fonts/SVN-Gilroy SemiBold.otf #SVN-Gilroy SemiBold</FontFamily>
    <FontFamily x:Key="GilroyThin">pack://application:,,,/Assets/Fonts/SVN-Gilroy Thin.otf #SVN-Gilroy Thin</FontFamily>
    <FontFamily x:Key="HelvetiaBold">pack://application:,,,/Assets/Fonts/HelveticaNeue-Bold.otf #Helvetica Neue Bold</FontFamily>
    <FontFamily x:Key="BebasNeue">pack://application:,,,/Assets/Fonts/Bebas Neue Pro Regular.otf #Bebas Neue Pro</FontFamily>
    <FontFamily x:Key="UTMBebas">pack://application:,,,/Assets/Fonts/UTM Bebas.ttf #UTM Bebas</FontFamily>

</ResourceDictionary>